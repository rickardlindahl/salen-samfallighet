{
  "name": "salen-samfallighetsforening",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "prettier": "prettier app/**/*.{ts,tsx}",
    "format:check": "npm run prettier -- --check",
    "format:fix": "npm run prettier -- --write",
    "lint": "eslint app",
    "lint:fix": "npm run lint -- --fix",
    "firebase:emulators:start": "npx firebase emulators:start --project development --only auth,firestore --import=./firebase-seed",
    "firebase:emulators:export": "npx firebase emulators:export --project development ./firebase-seed"
  },
  "dependencies": {
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/vercel": "^1.5.1",
    "@vercel/node": "^1.15.2",
    "firebase": "^9.8.1",
    "firebase-admin": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "firebase-tools": "^11.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  }
}
