{
  "extends": ["@remix-run/eslint-config", "@remix-run/eslint-config/node", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "prefer-const": "error",
    "no-unused-vars": ["error", { "vars": "all", "args": "all", "ignoreRestSiblings": false, "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" }]
  }
}
